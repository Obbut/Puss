// CustomStringConvertible.ejs

<% if (serializable.instanceVariables.map(v => v.name).includes("description")) {
	%>// Did not generate a CustomStringConvertible implementation because it would conflict with the existing `description` variable on <%- serializable.name %><%
} else { %>
extension <%- serializable.name %> : CustomStringConvertible {
	public var description: String {
	<% if (serializable.kind == "class") { %>
		return "<%- serializable.name %><\(ObjectIdentifier(self).hashValue),\((self.serialize() as Document).makeExtendedJSON(typeSafe: false).serializedString())>"
	<% } else { %>
		return "<%- serializable.name %><" + (self.serialize() as Document).makeExtendedJSON(typeSafe: false).serializedString() + ">"
	<% } %>
	}
}
<% } %>