// MARK: Serializable.ejs
<%
if (serializable.kind == "class" || serializable.kind == "struct") {
	%><%- include('SerializableStructClass', {serializable}) %><%
} else if (serializable.kind == "enum") {
	%><%- include('SerializableEnum', {serializable}) %><%
} else if (serializable.kind == "protocol") {
	%><%- include('SerializableProtocol', {serializable}) %><%
} else {
	%><%- meowError(`Type of kind '${serializable.kind}' named ${serializable.name} of type '${serializable.typeName}' unknown to Meow. Cannot generate Serializable implementation. (${serializable})`) %><%
}
%>
