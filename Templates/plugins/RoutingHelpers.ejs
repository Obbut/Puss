extension <%-serializable.name%> {
	public func `init`(from string: String) throws -> <%-serializable.name%>? {
		let objectId = try ObjectId(string)

		return try <%- serializable.name %>.findOne("_id" == objectId)
	}

	<% serializable.variables.forEach(variable => { -%>
		<%_ include('RouteInitializationHelpers', {variable}) -%>
	<% }); %>
}
