// MARK: Migrator.ejs

extension Migrator {
	<%
	types.all.find(t => t.name == "Migrator").instanceMethods.filter(m => m.parameters[0] && m.parameters[0].name == "property" && m.parameters[0].unwrappedTypeName == "String").forEach(method => {
		%>
		public func <%- method.shortName %>(<%_ %><%- method.parameters.map(p => `${p.argumentLabel ? p.argumentLabel + " " : ""}${p.name == p.argumentLabel ? "" : p.name}: ${p.name == "property" ? "M.Key" : p.typeName.name}`).join(', ') %>) {
			self.<%- method.callName %>(<%- method.parameters.map(p => `${p.argumentLabel ? p.argumentLabel + ": " : ""}${p.name}${p.name == "property" ? ".keyString" : ""}`).join(", ")%>)
		}
		<%
	});
	%>
}